{
  "manifest_version": 3,
  "name": "Auto-Refresh Prolific",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting",
    "alarms",
    "activeTab"
  ],
  "host_permissions": [
    "https://app.prolific.com/studies"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
